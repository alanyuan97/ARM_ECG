module node14_1(A0,A1,N1);
	input [31:0] A0;
	input [31:0] A1;
	output N1;  
	
	reg N1;

	parameter [31:0] W0=32'b01000000000000000000000000000000;   
	parameter [31:0] W1=32'b01000000000000000000000000000000;   
	wire [31:0] in0;
	wire [31:0] in1;    
	wire [31:0] sum1;

	float_mult mult0(     
		.x(A0),
		.y(W0),
		.z(in0),
		); 
		
	float_mult mult1(     
		.x(A1),
		.y(W1),
		.z(in1),
		);                  
		


	float_adder add0(
		.a(in0),
		.b(in1),
		.Out(sum1),                 
		.Out_test(),
		.shift(),
		.c_out(),
		);                   
		
		
		
		

always@(*)             //this sigmoid is a huge approximation, I just use it as a threshold detection
	begin						//because the network is trained this should work
		if(sum1[31]==0)
			N1=1; //if the input is 0 or larger the neuron will go high
		else
			N1=0;
	end
endmodule 